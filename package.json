{
    "name": "fox-tamagotchi",
    "version": "1.0.0",
    "description": "A mini tamagotchi replica game build using HTML/CSS/JS.",
    "main": "src/index.html",
    "scripts": {
        "build": "parcel build src/index.html",
        "dev": "parcel src/index.html",
        "start": "serve -s build",
        "format": "prettier --ignore-path ./.gitignore --write \"./**/*.{html,json,js,ts,css}\"",
        "format:check": "prettier --ignore-path ./.gitignore --check \"./**/*.{html,json,js,ts,css}\"",
        "lint": "eslint --ignore-path ./.gitignore --fix \"./**/*.{js,ts}\"",
        "lint:check": "eslint --ignore-path ./.gitignore --quiet \"./**/*.{js,ts}\"",
        "test": "echo \"Error: no test specified\" && exit 1",
        "heroku-postbuild": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kurnisurni/fox-tamagotchi.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/kurnisurni/fox-tamagotchi/issues"
    },
    "homepage": "https://github.com/kurnisurni/fox-tamagotchi#readme",
    "devDependencies": {
        "eslint": "^7.19.0",
        "eslint-config-prettier": "^7.2.0",
        "eslint-plugin-import": "^2.22.1",
        "parcel-bundler": "^1.12.4",
        "prettier": "^2.2.1"
    },
    "browserslist": [
        "last 2 Chrome versions"
    ],
    "optionalDependencies": {
        "fsevents": "^2.3.2"
    },
    "dependencies": {
        "serve": "^11.3.2"
    }
}